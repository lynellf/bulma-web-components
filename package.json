{
  "name": "bulma-web-components",
  "version": "1.0.0",
  "description": "Semantic Markup for the Bulma CSS Framework",
  "main": "webpack.config.dev.js",
  "directories": {
    "test": "tests"
  },
  "jest": {
    "preset": "jest-puppeteer"
  },
  "scripts": {
    "test": "jest --watch",
    "build": "webpack --config webpack.config.prod.js",
    "start": "concurrently \"webpack-dev-server --config webpack.config.dev.js\" \"npm run test\" \"npm run storybook\"",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lynellf/bulma-web-components.git"
  },
  "keywords": [
    "bulma",
    "css",
    "web",
    "components"
  ],
  "author": "Ezell Frazier",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lynellf/bulma-web-components/issues"
  },
  "homepage": "https://github.com/lynellf/bulma-web-components#readme",
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@babel/runtime": "^7.6.2",
    "@storybook/polymer": "^5.2.1",
    "@testing-library/dom": "^6.4.1",
    "@types/jest": "^24.0.18",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "bulma": "^0.7.5",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^4.1.2",
    "core-js": "^3.2.1",
    "font-awesome": "^4.7.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "jest-puppeteer": "^4.3.0",
    "jsdom": "^16.5.0",
    "lit-element": "^2.2.1",
    "polymer-webpack-loader": "^2.0.3",
    "puppeteer": "^1.20.0",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.1",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "emotion": "^10.0.17"
  }
}
